{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Opacity",
    "icons": {
    "48": "icons/images.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/images.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://*.mozilla.org/*"],
            "js": ["opacity.js"]
        }
    ]   
}
